<view class="page">
    <view class="page__hd">
        <view class="page__title">FabButton</view>
        <view class="page__desc">浮动按钮</view>
    </view>
    <view class="page__bd">
        <wux-fab-button
            position="topLeft"
            theme="success"
            direction="horizontal"
            reverse
            buttons="{{ buttons }}"
            bind:change="onChange"
            bind:click="onClick"
            bind:contact="onContact"
            bind:getuserinfo="onGotUserInfo"
        />

        <wux-fab-button
            position="topRight"
            theme="success"
            direction="vertical"
            reverse
            buttons="{{ buttons }}"
            bind:change="onChange"
            bind:click="onClick"
            bind:contact="onContact"
            bind:getuserinfo="onGotUserInfo"
        />

        <wux-fab-button
            position="bottomLeft"
            theme="success"
            direction="vertical"
            buttons="{{ buttons }}"
            bind:change="onChange"
            bind:click="onClick"
            bind:contact="onContact"
            bind:getuserinfo="onGotUserInfo"
        />

        <wux-fab-button
            position="center"
            theme="success"
            direction="circle"
            buttons="{{ buttons }}"
            bind:change="onChange"
            bind:click="onClick"
            bind:contact="onContact"
            bind:getuserinfo="onGotUserInfo"
        />

        <wux-fab-button
            position="bottomRight"
            theme="success"
            direction="horizontal"
            buttons="{{ buttons }}"
            bind:change="onChange"
            bind:click="onClick"
            bind:contact="onContact"
            bind:getuserinfo="onGotUserInfo"
        />
    </view>
</view>
