<wux-popup-select
    id="wux-popup-select"
    prefixCls="{{ prefixCls }}"
    value="{{ value }}"
    options="{{ options }}"
    multiple="{{ multiple }}"
    max="{{ max }}"
    toolbar="{{ toolbar }}"
    visible="{{ visible }}"
    controlled
    disabled
    bindcancel="onCancel"
    bindconfirm="onConfirm"
    bindvisibleChange="onVisibleChange"
    bindvalueChange="onValueChange"
/>
